{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "dest": "http://localhost:8000/api/:splat",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "env": {
    "DB_URL": "http://localhost:8000"
  },
  "dev": {
    "proxy": {
      "/api": "http://localhost:8000"
    }
  }
}
